<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.yavski.fabmenu.samples">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="io.github.yavski.fabspeeddial.samples.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="io.github.yavski.fabspeeddial.samples.PositionSampleActivity" />
        <activity android:name="io.github.yavski.fabspeeddial.samples.GroupStyleSampleActivity" />
        <activity android:name="io.github.yavski.fabspeeddial.samples.IndividualStyleSampleActivity" />
        <activity android:name="io.github.yavski.fabspeeddial.samples.EventsSampleActivity" />
        <activity android:name="io.github.yavski.fabspeeddial.samples.MenuItemsSampleActivity" />
        <activity android:name="io.github.yavski.fabspeeddial.samples.CustomizationSampleActivity" />

    </application>

</manifest>
